{
	"name": "remote-procedure-call",
	"version": "0.0.4",
	"publishConfig": {
		"access": "public"
	},
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			}
		},
		"./curried": {
			"require": {
				"types": "./dist/curried/index.d.ts",
				"default": "./dist/curried/index.cjs"
			},
			"import": {
				"types": "./dist/curried/index.d.ts",
				"default": "./dist/curried/index.mjs"
			}
		},
		"./proxied": {
			"require": {
				"types": "./dist/proxied/index.d.ts",
				"default": "./dist/proxied/index.cjs"
			},
			"import": {
				"types": "./dist/proxied/index.d.ts",
				"default": "./dist/proxied/index.mjs"
			}
		}
	},
	"files": [
		"dist",
		"src"
	],
	"devDependencies": {
		"pkgroll": "^2.0.2",
		"prettier": "^3.2.5",
		"set-defaults": "^0.0.2",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"named-logs": "^0.2.2",
		"promise-throttle": "^1.1.2",
		"vitest": "^2.1.1"
	},
	"scripts": {
		"prepare": "set-defaults .vscode",
		"build": "pkgroll",
		"dev": "pkgroll --watch",
		"format:check": "prettier --check .",
		"format": "prettier --write ."
	}
}
